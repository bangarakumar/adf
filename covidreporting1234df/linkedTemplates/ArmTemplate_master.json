{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidreporting1234df"
		},
		"ls_covidreporing_dl_sa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covidreporing_dl_sa'"
		},
		"ls_covidreporintg_ablob_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covidreporintg_ablob_sa'"
		},
		"ls_databrics_server_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databrics_server'"
		},
		"ls_http_etdc_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_http_etdc'"
		},
		"ls_sql_covid_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_covid_db'"
		},
		"ls_covidreporing_dl_sa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreporting123dlsa.dfs.core.windows.net/"
		},
		"ls_http_etdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"ls_http_etdc_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "bangarakumar"
		},
		"ls_sql_covid_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "covid-srv1234.database.windows.net"
		},
		"ls_sql_covid_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid-db"
		},
		"ls_sql_covid_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin1234"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidreporting1234df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covidreporing_dl_sa_accountKey": {
						"value": "[parameters('ls_covidreporing_dl_sa_accountKey')]"
					},
					"ls_covidreporintg_ablob_sa_connectionString": {
						"value": "[parameters('ls_covidreporintg_ablob_sa_connectionString')]"
					},
					"ls_databrics_server_accessToken": {
						"value": "[parameters('ls_databrics_server_accessToken')]"
					},
					"ls_http_etdc_password": {
						"value": "[parameters('ls_http_etdc_password')]"
					},
					"ls_sql_covid_db_password": {
						"value": "[parameters('ls_sql_covid_db_password')]"
					},
					"ls_covidreporing_dl_sa_properties_typeProperties_url": {
						"value": "[parameters('ls_covidreporing_dl_sa_properties_typeProperties_url')]"
					},
					"ls_http_etdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_etdc_properties_typeProperties_url')]"
					},
					"ls_http_etdc_properties_typeProperties_userName": {
						"value": "[parameters('ls_http_etdc_properties_typeProperties_userName')]"
					},
					"ls_sql_covid_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_server')]"
					},
					"ls_sql_covid_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_database')]"
					},
					"ls_sql_covid_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_covid_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covidreporting1234df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covidreporting1234df_ArmTemplate_0')]"
			]
		}
	]
}