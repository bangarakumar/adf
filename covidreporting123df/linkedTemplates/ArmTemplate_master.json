{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidreporting123df"
		},
		"ls_covidreporing_dl_sa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covidreporing_dl_sa'"
		},
		"ls_covidreporintg_ablob_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covidreporintg_ablob_sa'"
		},
		"ls_http_etdc_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_http_etdc'"
		},
		"ls_covidreporing_dl_sa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreporting123desa.dfs.core.windows.net/"
		},
		"ls_http_etdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"ls_http_etdc_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "bangarakumar"
		},
		"tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_relativeSourceUrl": {
			"type": "string",
			"defaultValue": "bangarakumar/covid19/blob/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_sorceFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidreporting123df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covidreporing_dl_sa_accountKey": {
						"value": "[parameters('ls_covidreporing_dl_sa_accountKey')]"
					},
					"ls_covidreporintg_ablob_sa_connectionString": {
						"value": "[parameters('ls_covidreporintg_ablob_sa_connectionString')]"
					},
					"ls_http_etdc_password": {
						"value": "[parameters('ls_http_etdc_password')]"
					},
					"ls_covidreporing_dl_sa_properties_typeProperties_url": {
						"value": "[parameters('ls_covidreporing_dl_sa_properties_typeProperties_url')]"
					},
					"ls_http_etdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_etdc_properties_typeProperties_url')]"
					},
					"ls_http_etdc_properties_typeProperties_userName": {
						"value": "[parameters('ls_http_etdc_properties_typeProperties_userName')]"
					},
					"tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_relativeSourceUrl": {
						"value": "[parameters('tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_relativeSourceUrl')]"
					},
					"tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_sorceFileName": {
						"value": "[parameters('tr_ingest_ecdc_data_properties_pl_ingest_cases_deaths_parameters_sorceFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}